- var showBack = true;
- var title = "Sets by User";
- var description = "View Created by This User";
doctype html
html(lang="en")
    include includes/head
    body.mdc-typography
        include includes/navbar
        main.mdc-top-app-bar--fixed-adjust
            div#home.page
                h1 Sets by User - 
                    span.field-user-name
                .set-container(data-empty="This user has not made any sets")
                button.mdc-button.mdc-button--outlined(style="display: block; margin-top: 0.5rem;" hidden)
                    span.mdc-button__ripple
                    span.mdc-button__label Load More
        include includes/footer
        script(src="js/user-sets.js" type="module")